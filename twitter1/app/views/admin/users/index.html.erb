<strong>User details </strong>

<table class="table">
	<thread>
  <tr>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
  </tr>
</thread>

  <%@user.each do |user|%>
<tr>
 <td><%=user.name%></td>
 <td><%=user.username%></td>
 <td><%=user.email%></td>
 <td><%= link_to admin_user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this user?" } do %>
                <span class="icon"><i class="fa fa-trash-o" aria-hidden="true"></i></span>
            <% end %></td>
<%end%>
</tr>
</table>



